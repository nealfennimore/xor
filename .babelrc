{
    "presets": [
        ["@babel/env", {
            "modules": false
        }]
    ],
    "env": {
        "cjs": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ],
            "ignore": [
                "*/__tests__"
            ]
        },
        "es": {
            "ignore": [
                "*/__tests__"
            ]
        },
        "test": {
            "plugins": [
                "@babel/plugin-transform-modules-commonjs"
            ]
        }
    }
}
